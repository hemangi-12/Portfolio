{
    "version": 2,
    "builds": [
      {
        "src": "server.js",  
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"  
      }
    ],
    "env": {
      "EMAIL_USER": "@email_user",
      "EMAIL_PASS": "@email_pass"
    }
  }
  
  